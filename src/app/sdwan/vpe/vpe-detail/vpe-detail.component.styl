.right-detail
  padding 20px
  li
    line-height 28px
    .detail-title
      font-weight: bold
      margin-left: -10px
    .text
      float right
      color: #00c1e0
  .interfaceBox-last
    border-bottom: 1px dashed #aaa
  .detail-split-line
    margin: 10px -20px
    height: 1px
    background-color: #ccc
.label
  width 40px
  height 25px
  line-height 25px
  text-align center
  color #fff!important
  border-radius: 0.25em
  font-weight: bold
.label-success
  background: #5cb85c
.label-danger
  background: #d9534f
.label-info
  background: #f0ad4e
