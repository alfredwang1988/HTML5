.year-table
  margin: -1px
  width: 100%
  font-size: 12px
  border-collapse: collapse
  td
    padding: 20px 3px
    text-align: center
    cursor: pointer
    .cell
      display: block
      margin: 0 auto
      width: 48px
      height: 32px
      line-height: 32px
      color: #5a5e66
    &.current
      .cell
        color: #409eff
